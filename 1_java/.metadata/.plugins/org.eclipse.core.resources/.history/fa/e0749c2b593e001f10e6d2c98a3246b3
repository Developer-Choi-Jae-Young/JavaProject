package kh.study.cjy.control;

public class FactoryMethodControl {
	public IControl createContorl(String type){
		IControl control = null;
        switch (type){
            case "USER":
            	control = new UserControl();
                break;

            case "Samsung":
            	control = new SamsungMouse();
                break;
                default:
                	break;
        }

        return control;
    }
}
