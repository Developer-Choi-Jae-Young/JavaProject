package etc;

public class Banner {
	public void print() {
		System.out.println(
				".----------------.  .----------------.  .----------------.  .----------------.   .----------------.  .----------------.  .----------------.  .----------------.  .----------------.");
		System.out.println(
				"| .--------------. || .--------------. || .--------------. || .--------------. | | .--------------. || .--------------. || .--------------. || .--------------. || .--------------. |");
		System.out.println(
				"| |    _______   | || |  _________   | || |   _____      | || |  _________   | | | |    _______   | || |  _________   | || | _____  _____ | || |  ________    | || |  ____  ____  | |");
		System.out.println(
				"| |   /  ___  |  | || | |_   ___  |  | || |  |_   _|     | || | |_   ___  |  | | | |   /  ___  |  | || | |  _   _  |  | || ||_   _||_   _|| || | |_   ___ `.  | || | |_  _||_  _| | |");
		System.out.println(
				"| |  |  (__ \\_|  | || |   | |_  \\_|  | || |    | |       | || |   | |_  \\_|  | | | |  |  (__ \\_|  | || | |_/ | | \\_|  | || |  | |    | |  | || |   | |   `. \\ | || |   \\ \\  / /   | |");
		System.out.println(
				"| |   '.___`-.   | || |   |  _|  _   | || |    | |   _   | || |   |  _|      | | | |   '.___`-.   | || |     | |      | || |  | '    ' |  | || |   | |    | | | || |    \\ \\/ /    | |");
		System.out.println(
				"| |  |`\\____) |  | || |  _| |___/ |  | || |   _| |__/ |  | || |  _| |_       | | | |  |`\\____) |  | || |    _| |_     | || |   \\ `--' /   | || |  _| |___.' / | || |    _|  |_    | |");
		System.out.println(
				"| |  |_______.'  | || | |_________|  | || |  |________|  | || | |_____|      | | | |  |_______.'  | || |   |_____|    | || |    `.__.'    | || | |________.'  | || |   |______|   | |");
		System.out.println(
				"| |              | || |              | || |              | || |              | | | |              | || |              | || |              | || |              | || |              | |");
		System.out.println(
				"| '--------------' || '--------------' || '--------------' || '--------------' | | '--------------' || '--------------' || '--------------' || '--------------' || '--------------' |");
		System.out.println(
				"'----------------'  '----------------'  '----------------'  '----------------'   '----------------'  '----------------'  '----------------'  '----------------'  '----------------'");
		
		System.out.println();
		System.out.println();
		System.out.println();
		
		String[] in = {"1", "2", "6" };
		
		int count = 0;
		int max = 0;
        int eyes = 0;
		
        for(int i = 0; i < in.length; i++) {
            for(int j = 0; j < in.length; j++) {            	
            	if(in[i] == in[j]) {
            		count++;
            	}
            }
            
            if(max < count) {
            	max = count;
            	eyes = Integer.parseInt(in[i]);
            } else {
            	eyes = eyes > Integer.parseInt(in[i]) ? eyes : Integer.parseInt(in[i]);
            }
            count = 0;
        }
        
        System.out.println(eyes);
        
        int price = 0;
        switch(max) {
            case 3 :
                price = 10000 + (eyes) * 100;
                break;
            case 2:
                price = 1000 + (eyes) * 100;
                break;
            default :
                price = (eyes) * 100;
                break;
        }
        
        System.out.println(price);
	}
}
