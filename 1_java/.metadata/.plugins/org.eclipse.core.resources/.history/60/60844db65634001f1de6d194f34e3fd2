package etc;

public class Banner {
	public void print() {
		System.out.println(
				".----------------.  .----------------.  .----------------.  .----------------.   .----------------.  .----------------.  .----------------.  .----------------.  .----------------.");
		System.out.println(
				"| .--------------. || .--------------. || .--------------. || .--------------. | | .--------------. || .--------------. || .--------------. || .--------------. || .--------------. |");
		System.out.println(
				"| |    _______   | || |  _________   | || |   _____      | || |  _________   | | | |    _______   | || |  _________   | || | _____  _____ | || |  ________    | || |  ____  ____  | |");
		System.out.println(
				"| |   /  ___  |  | || | |_   ___  |  | || |  |_   _|     | || | |_   ___  |  | | | |   /  ___  |  | || | |  _   _  |  | || ||_   _||_   _|| || | |_   ___ `.  | || | |_  _||_  _| | |");
		System.out.println(
				"| |  |  (__ \\_|  | || |   | |_  \\_|  | || |    | |       | || |   | |_  \\_|  | | | |  |  (__ \\_|  | || | |_/ | | \\_|  | || |  | |    | |  | || |   | |   `. \\ | || |   \\ \\  / /   | |");
		System.out.println(
				"| |   '.___`-.   | || |   |  _|  _   | || |    | |   _   | || |   |  _|      | | | |   '.___`-.   | || |     | |      | || |  | '    ' |  | || |   | |    | | | || |    \\ \\/ /    | |");
		System.out.println(
				"| |  |`\\____) |  | || |  _| |___/ |  | || |   _| |__/ |  | || |  _| |_       | | | |  |`\\____) |  | || |    _| |_     | || |   \\ `--' /   | || |  _| |___.' / | || |    _|  |_    | |");
		System.out.println(
				"| |  |_______.'  | || | |_________|  | || |  |________|  | || | |_____|      | | | |  |_______.'  | || |   |_____|    | || |    `.__.'    | || | |________.'  | || |   |______|   | |");
		System.out.println(
				"| |              | || |              | || |              | || |              | | | |              | || |              | || |              | || |              | || |              | |");
		System.out.println(
				"| '--------------' || '--------------' || '--------------' || '--------------' | | '--------------' || '--------------' || '--------------' || '--------------' || '--------------' |");
		System.out.println(
				"'----------------'  '----------------'  '----------------'  '----------------'   '----------------'  '----------------'  '----------------'  '----------------'  '----------------'");
		
		System.out.println();
		System.out.println();
		System.out.println();
		
		String str = "3 3 6";
		
		
		for(String s : str.split(" ")) {
			System.out.println(s);
		}
	}
}
