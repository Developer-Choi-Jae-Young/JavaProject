package kh.study.cjy.run;

import java.util.Scanner;

import kh.study.cjy.view.View;

public class Run {

	public static void main(String[] args) {		
		Scanner sc = new Scanner(System.in);
		
		String[] time = sc.nextLine().split(" ");
		int calcMinute = Integer.parseInt(time[1]) - 45;
		
		if(calcMinute < 0) {
			int calcHour = Integer.parseInt(time[0]) - 1;
			if(calcHour < 0) {
				calcHour = 24 + calcHour;
			}
			
			calcMinute = 60 - Math.abs(calcMinute);
		}
		
		System.out.println();
		
		//new View();
		
