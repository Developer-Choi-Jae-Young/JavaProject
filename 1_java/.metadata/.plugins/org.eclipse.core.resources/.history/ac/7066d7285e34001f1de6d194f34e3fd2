package kh.study.cjy.run;

import java.util.Scanner;

import kh.study.cjy.view.View;

public class Run {

	public static void main(String[] args) {		
		Scanner sc = new Scanner(System.in);
		
		String[] time = sc.nextLine().split(" ");
		String endTime = sc.nextLine();
		
		int calcTime = Integer.parseInt(endTime) + Integer.parseInt(time[1]);
		
		int calcHour = Integer.parseInt(time[0]) + (calcTime / 60);
		int calcMinute = calcTime % 60;
		
		System.out.println(calcHour + " " + calcMinute);
		
		//new View();
		
